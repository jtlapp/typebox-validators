{
  "name": "typebox-validators",
  "version": "0.1.0",
  "author": "Joseph T. Lapp <arachnojoe@gmail.com>",
  "license": "MIT",
  "description": "TypeBox validators with custom errors, safe multi-tier error handling, discriminated unions",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtlapp/typebox-validators"
  },
  "keywords": [
    "typebox",
    "validation",
    "errors",
    "error handling",
    "client",
    "server",
    "security",
    "secure",
    "discriminated unions"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "/index.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "rm -rdf dist && tsc && tsc -p tsconfig.cjs.json",
    "build-docs": "typedoc --options ./typedoc.js ./src/index.ts",
    "build-all": "pnpm build && pnpm build-docs",
    "test": "jest",
    "test-perf": "ts-node src/test/key-iteration-performance.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.28.14"
  }
}
